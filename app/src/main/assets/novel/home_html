//定义接口
var Api = {
    Info:{
        img: 'cover'
        ,path: "/hy/"
        ,cover: /cover\/(.*?)(\/|\.)/i
        ,book: /\/hy\/(.*?)(\/|\.)/i
        ,chapter: /\/hy\/(.*?)\/(.*?)(\/|\.)/i
    },
    Open: function (book, chapter) {
        //打开书籍
        var url = location.origin + this.Info.path + book;
        if (chapter) url += "/" + chapter;
        location.href = url + ".html";
    }
};//定义接口
